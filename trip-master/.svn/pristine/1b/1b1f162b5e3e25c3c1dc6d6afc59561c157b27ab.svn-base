package com.newroad.tripmaster.dao.pojo.trip;

import java.io.Serializable;
import java.util.List;

import org.mongodb.morphia.annotations.Embedded;

@Embedded
public class TravelHotel implements Serializable{

  
  /**
   * 
   */
  private static final long serialVersionUID = -1037875747220057644L;

  private String hotelName;
  
  private List<String> roomType;
  
  private Boolean cancelRequirement;
  
  private String service;
  
  private Boolean canWIFI;
  
  private Boolean canParking;

  public String getHotelName() {
    return hotelName;
  }

  public void setHotelName(String hotelName) {
    this.hotelName = hotelName;
  }

  public List<String> getRoomType() {
    return roomType;
  }

  public void setRoomType(List<String> roomType) {
    this.roomType = roomType;
  }

  public Boolean getCancelRequirement() {
    return cancelRequirement;
  }

  public void setCancelRequirement(Boolean cancelRequirement) {
    this.cancelRequirement = cancelRequirement;
  }

  public String getService() {
    return service;
  }

  public void setService(String service) {
    this.service = service;
  }

  public Boolean getCanWIFI() {
    return canWIFI;
  }

  public void setCanWIFI(Boolean canWIFI) {
    this.canWIFI = canWIFI;
  }

  public Boolean getCanParking() {
    return canParking;
  }

  public void setCanParking(Boolean canParking) {
    this.canParking = canParking;
  }
  
  
}
