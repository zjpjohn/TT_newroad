package com.newroad.tripmaster.dao.pojo.trip;

import java.io.Serializable;
import java.util.List;

import org.mongodb.morphia.annotations.Embedded;

@Embedded
public class TripDayRoute implements Serializable {

  /**
   * 
   */
  private static final long serialVersionUID = 7759365951999813647L;

  private String tripRouteId;

  private String dayStartCity;

  private String dayStayCity;

  private List<POIRoute> poiRouteList;
  
  private POIRoute[] poiRouteArray;

  public TripDayRoute(String tripRouteId, List<POIRoute> poiRouteList) {
    super();
    this.tripRouteId = tripRouteId;
    this.poiRouteList = poiRouteList;
  }

  public String getTripRouteId() {
    return tripRouteId;
  }

  public void setTripRouteId(String tripRouteId) {
    this.tripRouteId = tripRouteId;
  }

  public String getDayStartCity() {
    return dayStartCity;
  }

  public void setDayStartCity(String dayStartCity) {
    this.dayStartCity = dayStartCity;
  }

  public String getDayStayCity() {
    return dayStayCity;
  }

  public void setDayStayCity(String dayStayCity) {
    this.dayStayCity = dayStayCity;
  }

  public List<POIRoute> getPoiRouteList() {
    return poiRouteList;
  }

  public void setPoiRouteList(List<POIRoute> poiRouteList) {
    this.poiRouteList = poiRouteList;
  }

  
  public POIRoute[] getPoiRouteArray() {
    return poiRouteArray;
  }

  
  public void setPoiRouteArray(POIRoute[] poiRouteArray) {
    this.poiRouteArray = poiRouteArray;
  }

  
}
