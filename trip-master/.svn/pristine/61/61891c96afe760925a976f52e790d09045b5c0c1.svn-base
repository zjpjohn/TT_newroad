package com.newroad.tripmaster.dao;

import org.bson.types.ObjectId;
import org.mongodb.morphia.Datastore;
import org.mongodb.morphia.Key;
import org.mongodb.morphia.dao.BasicDAO;
import org.slf4j.Logger;
import org.slf4j.LoggerFactory;

import com.newroad.tripmaster.dao.pojo.trip.TravelPOI;

public class TravelUserPOIDao extends BasicDAO<TravelPOI, ObjectId>{

  private static Logger logger = LoggerFactory.getLogger(TravelUserPOIDao.class);
  
  protected TravelUserPOIDao(Datastore ds) {
    super(ds);
    // TODO Auto-generated constructor stub
  }
  
  public Object saveTravelPOI(TravelPOI travelPOI) {
    Key<TravelPOI> key = getDs().save(travelPOI);
    return key.getId();
  }
}
