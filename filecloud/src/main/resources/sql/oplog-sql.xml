	<update id="updateOpLog" collection="ln_oplog">
		<porperty name="param" class="java.util.Map">
			<value column="dataID" name="dataID" value="" />
			<value column="userID" name="userID" value="" />
		</porperty>
		<property name="action" class="java.util.Map">
