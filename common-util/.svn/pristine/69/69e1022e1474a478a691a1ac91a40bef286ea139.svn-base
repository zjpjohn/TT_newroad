package com.newroad.util.cosure;

/**
 * @info  : 可对比的 
 * @author: xiangping_yu
 * @data  : 2013-6-9
 * @since : 1.5
 */
public interface Comparable<T, V, V2, T2> {
	/**
	 * @param V 用于对比的类型
	 * @param V2 用于对比的类型
	 * @param T2 回调参数类型
	 * @param T  结果数据
	 */
	void compare(T t ,V v1, V2 v2, CallBack<V, T2> callback);
}
